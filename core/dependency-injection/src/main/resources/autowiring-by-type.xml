<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="logger" class="com.asaunin.xml.autowiring.by.type.Logger" autowire="byType"/>

    <bean id="fileLogger" class="com.asaunin.xml.autowiring.FileWriter"/>
    <bean id="consoleLogger" class="com.asaunin.xml.autowiring.ConsoleWriter"/>

</beans>