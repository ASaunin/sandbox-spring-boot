<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
       default-autowire="byType" default-autowire-candidates="fileLogger,consoleLogger">

    <bean id="logger" class="xml.autowiring.by.defolt.Logger"/>

    <bean id="fileLogger" class="xml.autowiring.FileWriter"/>       <!-- Set primary="true" property instead of default-autowire-candidates -->
    <bean id="fileLogger2" class="xml.autowiring.FileWriter"/>      <!-- Set primary="true" property instead of default-autowire-candidates -->
    <bean id="consoleLogger" class="xml.autowiring.ConsoleWriter"/>
    <bean id="consoleLogger2" class="xml.autowiring.ConsoleWriter"/>

</beans>